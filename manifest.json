{
  "manifest_version": 3,
  "name": "LinkedIn DOM Extractor",
  "version": "1.0",
  "description": "Extract LinkedIn page DOM for analysis",
  "permissions": ["activeTab", "downloads", "scripting"],
  "host_permissions": ["https://www.linkedin.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "LinkedIn DOM Extractor"
  }
}

